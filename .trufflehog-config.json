{
  "detectors": [
    {
      "name": "GitHub"
    },
    {
      "name": "AWS"
    },
    {
      "name": "Slack"
    },
    {
      "name": "GoogleAPIKey"
    },
    {
      "name": "NPM"
    },
    {
      "name": "Docker"
    },
    {
      "name": "PrivateKey"
    }
  ],
  "exclude": {
    "paths": [
      "**/node_modules/**",
      "**/vendor/**",
      "**/*.lock",
      "**/package-lock.json",
      "**/yarn.lock",
      "**/.git/**",
      "**/test/**",
      "**/tests/**",
      "**/__tests__/**",
      "**/dist/**",
      "**/build/**",
      "**/*.min.js",
      "**/*.map",
      "**/coverage/**"
    ]
  }
}
